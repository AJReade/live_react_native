{
  "name": "live_react_native",
  "version": "0.1.0",
  "description": "React Native adapter for Phoenix LiveView reactivity",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint js/**/*.{ts,tsx}",
    "lint:fix": "eslint js/**/*.{ts,tsx} --fix",
    "type-check": "tsc --noEmit",
    "example": "cd example && npm start"
  },
  "keywords": [
    "react-native",
    "phoenix",
    "liveview",
    "websocket",
    "real-time",
    "elixir"
  ],
  "author": "LiveReact Native Team",
  "license": "MIT",
  "peerDependencies": {
    "react": "^18.2.0 || ^19.0.0",
    "react-native": "^0.73.0 || ^0.79.0"
  },
  "dependencies": {
    "phoenix": "^1.7.10"
  },
  "devDependencies": {
    "@types/react": "~18.2.79",
    "@types/react-native": "~0.73.0",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.3"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mrdotb/live_react_native.git"
  },
  "bugs": {
    "url": "https://github.com/mrdotb/live_react_native/issues"
  },
  "homepage": "https://github.com/mrdotb/live_react_native#readme"
}
